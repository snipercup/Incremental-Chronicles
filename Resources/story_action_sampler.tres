[gd_resource type="NobodyWhoSampler" format=3 uid="uid://djcekom8vka8y"]

[resource]
use_grammar = true
gbnf_grammar = "root ::= \"{\" ws01 root-message \"}\" ws01
root-message ::= \"\\\"message\\\"\" \":\" ws01 string


value  ::= (object | array | string | number | boolean | null) ws

object ::=
  \"{\" ws (
    string \":\" ws value
    (\",\" ws string \":\" ws value)*
  )? \"}\"

array  ::=
  \"[\" ws01 (
            value
    (\",\" ws01 value)*
  )? \"]\"

string ::=
  \"\\\"\" (string-char)* \"\\\"\"

string-char ::= [^\"\\\\] | \"\\\\\" ([\"\\\\/bfnrt] | \"u\" [0-9a-fA-F] [0-9a-fA-F] [0-9a-fA-F] [0-9a-fA-F]) # escapes

number ::= integer (\".\" [0-9]+)? ([eE] [-+]? [0-9]+)?
integer ::= \"-\"? ([0-9] | [1-9] [0-9]*)
boolean ::= \"true\" | \"false\"
null ::= \"null\"

# Optional space: by convention, applied in this grammar after literal chars when allowed
ws ::= ([ \\t\\n] ws)?
ws01 ::= ([ \\t\\n])?"
